set nu
syntax on
set smartindent
set ruler
set expandtab
set shiftwidth=2
set tabstop=2
set cursorline
filetype plugin indent on
set noswapfile
set incsearch

map <F2> :bprev<CR>
map <F3> :bprev<CR>
set wildmenu
set hlsearch
set showcmd

call plug#begin('~/.vim/plugged')
Plug 'morhetz/gruvbox'
call plug#end()

colorscheme gruvbox
set background=dark
